{
  "name": "vite-tauri",
  "version": "0.0.0",
  "scripts": {
    "dev:elm": "elm-live src/Main.elm -p 4000 -d public/ -- --output public/index.html",
    "dev:tauri": "yarn tauri dev",
    "dev": "run-p dev:*",
    "build": "tsc --noEmit && parcel build index.html --out-dir dist",
    "serve": "parcel serve index.html --out-dir dist --port 4000"
  },
  "dependencies": {
    "@types/sharp": "^0.27.1",
    "tauri": "^0.14.1"
  },
  "devDependencies": {
    "elm-hot": "^1.1.6",
    "elm-live": "^4.0.2",
    "node-elm-compiler": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.2.4"
  }
}
